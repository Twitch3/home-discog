{
  "name": "home-discog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve & npm run start-server",
    "start-server": "nodemon --watch db-server/**/*.ts --exec ts-node db-server/main.ts",
    "build": "ng build & tsc ./db-server/main.ts --outDir ./dist/db-server/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "express": "^5.1.0",
    "lowdb": "^7.0.1",
    "musicbrainz-api": "^0.25.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.3.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}